[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "./gdextension/target/debug/libYarnSpinner_GDExtension.so"
linux.release.x86_64 =   "./gdextension/target/release/libYarnSpinner_GDExtension.so"
windows.debug.x86_64 =   "./gdextension/target/debug/YarnSpinner_GDExtension.dll"
windows.release.x86_64 = "./gdextension/target/release/YarnSpinner_GDExtension.dll"
macos.debug =            "./gdextension/target/debug/libYarnSpinner_GDExtension.dylib"
macos.release =          "./gdextension/target/release/libYarnSpinner_GDExtension.dylib"
macos.debug.arm64 =      "./gdextension/target/debug/libYarnSpinner_GDExtension.dylib"
macos.release.arm64 =    "./gdextension/target/release/libYarnSpinner_GDExtension.dylib"